{
  "name": "fair-launch-subgraph",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ fair-launch-subgraph",
    "create-local": "graph create --node http://localhost:8020/ fair-launch-subgraph",
    "remove-local": "graph remove --node http://localhost:8020/ fair-launch-subgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 fair-launch-subgraph",
    "test": "graph test",
    "fork": "make local-fork",
    "prepare-base-sepolia": "npx mustache networkConfig/base-sepolia.json subgraph.template.yaml > subgraph.yaml",
    "prepare-localhost": "npx mustache networkConfig/localhost.json subgraph.template.yaml > subgraph.yaml",
    "go-local": "yarn remove-local && yarn create-local && yarn codegen && yarn prepare-localhost && yarn build && yarn deploy-local"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.71.0",
    "@graphprotocol/graph-ts": "0.32.0",
    "mustache": "4.2.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}
